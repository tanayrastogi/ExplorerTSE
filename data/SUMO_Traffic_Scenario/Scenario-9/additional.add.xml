        <vType id="CarC" accel="3.0" decel="5.0"  length="5.0" minGap="1.5" maxSpeed="10.0"  sigma="0.5" probability="0.25"  color="yellow"/>
        <vType id="CarD" accel="2.0" decel="6.0"  length="5.0" minGap="1.5" maxSpeed="20.0"  sigma="0.5" probability="0.25"  color="green"/>
